#include "stdafx.h"
 #include <iostream>
 #include "conio.h"


 using namespace std;
 void main()
 {
 	float n1, n2, n3;
 	cout<< "Ingresar el valor de n1:";
 	cin>> n1;
 	cout<< "Ingresar el valor de n2:";
 	cin>> n2;
 	cout<< "Ingresar el valor de n3:";
 	cin>> n3;
 	if (n1>= n2)
 		if(n1 >= n3)
 			cout<<"El valor mayor es:"<<n1<<endl;
 		else 
 			cout<<"El valor mayor es:"<<n3<<endl;
 	else
 		if (n2>=n3)
 			cout<<"El mayor es:"<<n2<<endl;
 		else
 			cout<<"El mayor es:"<<n3<<endl;
 	getch ();


 }
